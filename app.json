{
  "expo": {
    "name": "Bryan's Mobile App",
    "scheme": "FirstReactNativeApp",
    "slug": "bryans_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/bronzeboat.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/animalspiral.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "package": "com.shootingutah.firstapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.shootingutah.firstapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/bronzeboat.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icons/bronzeboat.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}
